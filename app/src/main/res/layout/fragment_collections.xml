<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmentCollectionsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CollectionsFragment">

    <!-- TODO: Update blank fragment layout -->

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText"
        android:layout_width="318dp"
        android:layout_height="57dp"
        android:gravity="center"
        android:hint="Input number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.133" />

    <Button
        android:id="@+id/btnCollectionsFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935" />

    <TableLayout
        android:layout_width="391dp"
        android:layout_height="365dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.61">

        <TableRow
            android:id="@+id/trTitles"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvVoid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/tvArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ArrayList   "
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="LinkedList   "
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvACopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CopyOnWriteList"
                android:textSize="12sp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvAddingInTheBeginning"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Adding in the beginning" />

            <TextView
                android:id="@+id/tvAddingInTheBeginningArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvAddingInTheBeginningLinkedList"
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvAddingInTheBeginningCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvAddingInTheMiddle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Adding in the middle" />

            <TextView
                android:id="@+id/tvAddingInTheMiddleArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvAddingInTheMiddleLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvAddingInTheMiddleCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvAddingInTheEnd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Adding in the end" />

            <TextView
                android:id="@+id/tvAddingInTheEndArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvAddingInTheEndLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvAddingInTheEndCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvSearchByValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Search by value" />

            <TextView
                android:id="@+id/tvSearchByValueArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvSearchByValueLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvSearchByValueCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvRemovingInTheBeginning"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Removing in the beginning   " />

            <TextView
                android:id="@+id/tvRemovingInTheBeginningArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvRemovingInTheBeginningLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvRemovingInTheBeginningCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvRemovingInTheMiddle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Removing in the middle" />

            <TextView
                android:id="@+id/tvRemovingInTheMiddleArrayList"
                android:layout_width="46dp"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvRemovingInTheMiddleLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvRemovingInTheMiddleCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvRemovingInTheEnd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Removing in the end" />

            <TextView
                android:id="@+id/tvRemovingInTheEndArrayList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvRemovingInTheEndLinkedList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />

            <TextView
                android:id="@+id/tvRemovingInTheEndCopyOnWriteList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 ms" />
        </TableRow>

    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>